offset_left = -165.0
offset_top = -15.0
offset_right = 165.0
offset_bottom = 16.0
theme_override_font_sizes/font_size = 10
text = "• a cidadania (as pessoas têm direitos e deveres, e podem participar 
das decisões do país),"

[node name="Dialogo_9" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_9"]
position = Vector2(-18, 0)
