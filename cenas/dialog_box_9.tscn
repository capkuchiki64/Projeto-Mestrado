[node name="Label" type="Label" parent="Dialogo_11"]
layout_mode = 0
offset_left = -187.0
offset_top = -15.0
offset_right = 186.0
offset_bottom = 16.0
theme_override_font_sizes/font_size = 10
text = "Tudo isso forma a base do nosso país. E a dignidade da pessoa humana 
está no centro disso tudo: não dá pra ter cidadania de verdade sem respeito; "

[node name="Dialogo_12" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_12"]
position = Vector2(-18, 0)
scale = Vector2(1.2301258, 0.20502092)
texture = ExtResource("1_trh5l")

[node name="Label" type="Label" parent="Dialogo_12"]
layout_mode = 0
offset_left = -189.0
offset_top = -24.0
offset_right = 190.0
offset_bottom = 24.0
theme_override_font_sizes/font_size = 10
text = "não tem como valorizar o trabalho se quem trabalha não tem uma vida digna; 
não existe pluralismo se as pessoas não puderem se expressar com liberdade 
e respeito."

[node name="Dialogo_13" type="Control" parent="."]
