[node name="Label" type="Label" parent="Dialogo_9"]
layout_mode = 0
offset_left = -179.0
offset_top = -15.0
offset_right = 179.0
offset_bottom = 16.0
theme_override_font_sizes/font_size = 10
text = "• os valores sociais do trabalho e da livre iniciativa (todo tipo de 
trabalho deve ser valorizado e as pessoas têm liberdade pra empreender),"

[node name="Dialogo_10" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_10"]
