offset_left = -181.0
offset_top = -17.0
offset_right = 180.0
offset_bottom = 14.0
theme_override_font_sizes/font_size = 10
text = "Esse princípio da dignidade está ligado com outros fundamentos do Brasil, 
que também aparecem no artigo 1º da Constituição. São eles:"

[node name="Dialogo_7" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_7"]
position = Vector2(-18, 0)
scale = Vector2(1.2301258, 0.20502092)
texture = ExtResource("1_trh5l")

[node name="Label" type="Label" parent="Dialogo_7"]
layout_mode = 0
offset_left = -175.0
offset_top = -7.0
offset_right = 173.0
offset_bottom = 7.0
theme_override_font_sizes/font_size = 10
text = "• a soberania(o Brasil tem autonomia pra tomar suas próprias decisões),"

[node name="Dialogo_8" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_8"]
