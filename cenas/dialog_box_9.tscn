theme_override_font_sizes/font_size = 10
text = "Você já parou pra pensar que só por ser uma pessoa, você tem valor? 
Pois é, isso tem um nome importante: dignidade da pessoa humana. "

[node name="Dialogo_2" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_2"]
