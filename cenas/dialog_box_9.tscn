offset_top = -16.0
offset_right = 174.0
offset_bottom = 15.0
theme_override_font_sizes/font_size = 10
text = "Esse é um dos direitos mais importantes que existem, e tá garantido na 
Constituição Federal de 1988, no artigo 1º, inciso III. "

[node name="Dialogo_3" type="Control" parent="."]
visible = false
anchors_preset = 0
