offset_left = -181.0
offset_top = -24.0
offset_right = 180.0
offset_bottom = 24.0
theme_override_font_sizes/font_size = 10
text = "Basicamente, quer dizer que todos — independente de cor, classe social, 
religião, gênero, ou qualquer outra coisa — deve ser tratado com respeito, 
como alguém que tem valor só por ser humano."

[node name="Dialogo_4" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_4"]
position = Vector2(-18, 0)
scale = Vector2(1.2301258, 0.20502092)
texture = ExtResource("1_trh5l")

[node name="Label" type="Label" parent="Dialogo_4"]
layout_mode = 0
offset_left = -175.0
offset_top = -16.0
offset_right = 176.0
offset_bottom = 15.0
theme_override_font_sizes/font_size = 10
text = "Na vida real, isso significa que ninguém pode te humilhar,te tratar como 
inferior ou te negar direitos só porque você é diferente."

[node name="Dialogo_5" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_5"]
position = Vector2(-18, 0)
scale = Vector2(1.2301258, 0.20502092)
texture = ExtResource("1_trh5l")

[node name="Label" type="Label" parent="Dialogo_5"]
layout_mode = 0
offset_left = -185.0
offset_top = -16.0
offset_right = 184.0
offset_bottom = 15.0
theme_override_font_sizes/font_size = 10
text = "Também quer dizer que você merece viver com qualidade, estudar, 
trabalhar, ter acesso à saúde, segurança e tudo que garante uma vida digna."

[node name="Dialogo_6" type="Control" parent="."]
visible = false
anchors_preset = 0
offset_right = 40.0
offset_bottom = 40.0

[node name="Sprite2D2" type="Sprite2D" parent="Dialogo_6"]
